{% extends 'base.html.twig' %}

{% block content %}
<div class="row">
    <div class="col-xs-12">
        <h1>{{ user.firstName }}&nbsp;{{ user.lastName }}</h1>
        <dl>
            <dt>Email</dt>
            <dd>{{ user.email }}</dd>
            <dt>First Name</dt>
            <dd>{{ user.firstName }}</dd>
            <dt>Last Name</dt>
            <dd>{{ user.lastName }}</dd>
            <dt>Address</dt>
            <dd>{{ user.address|nl2br }}</dd>
        </dl>
    </div>
</div>

{% endblock %}

